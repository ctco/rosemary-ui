    "element-closest": "^2.0.2",
    "fuse.js": "^3.1.0",
    "immutable": "^3.7.6",
    "invariant": "^2.2.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "react-transition-group": "^1.2.0",
    "react-list": "^0.8.10",
    "react-overlays": "^0.8.3",
    "react-prop-types": "^0.4.0",
    "tether": "^1.4.0",
    "uncontrollable": "^4.1.0",
    "warning": "^3.0.0"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "buildGh-Pages"
    ],
    "noprompt": true
  }
}
