    "glob": "^7.1.2",
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.2.0",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "jquery": "^3.2.1",
    "jsdom": "11.5.1",
    "jshint": "^2.9.1",
    "jshint-loader": "^0.8.3",
    "mocha": "^4.0.1",
