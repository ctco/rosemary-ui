    "highlight.js": "^9.12.0",
    "hjs-webpack": "^9.2.0",
    "immutable": "^3.7.6",
    "invariant": "^2.2.1",
    "jsx-to-string": "^1.3.0",
    "lodash": "^4.11.1",
    "moment": "2.19.3",
    "prettier": "^1.7.4",
    "prop-types": "^15.6.0",
